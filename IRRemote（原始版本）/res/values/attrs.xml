<?xml version="1.0" encoding="utf-8"?>
<resources>
   
  
  
          
    <declare-styleable name="UIButton">
	    <attr name="title" format="string" />
	    <attr name="subtitle" format="string" />
	    <attr name="image" format="integer" />
  	</declare-styleable>
  	
    
      <declare-styleable name="DropDown">
        <attr name="arrowImage" format="integer" />
        <attr name="listBackgroundColor" format="integer" />
        <attr name="listDividerColor" format="integer" />
        <attr name="titleText" format="string" localization="suggested" />
        <attr name="titleColor" format="color" />
        <attr name="titleBackground" format="integer" />
        <attr name="android:entries" />
    </declare-styleable>
    
       <declare-styleable name="SwitchButton">
        <attr name="bmWidth" format="dimension"></attr>
        <attr name="bmHeight" format="dimension"></attr>
    </declare-styleable>
    
       
       <declare-styleable name="SlidingMenu">
        <attr name="mode">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="viewAbove" format="reference" />
        <attr name="viewBehind" format="reference" />
        <attr name="behindOffset" format="dimension" />
        <attr name="behindWidth" format="dimension" />
        <attr name="behindScrollScale" format="float" />
        <attr name="touchModeAbove">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="touchModeBehind">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="shadowDrawable" format="reference" />
        <attr name="shadowWidth" format="dimension" />
        <attr name="fadeEnabled" format="boolean" />
        <attr name="fadeDegree" format="float" />
        <attr name="selectorEnabled" format="boolean" />
        <attr name="selectorDrawable" format="reference" />
    </declare-styleable>
       
      <declare-styleable name="CircleButton">
        <attr name="cb_color" format="color" />
        <attr name="cb_pressedRingWidth" format="dimension" />
    </declare-styleable>
</resources>
